{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${mod_version}",
  "name": "${mod_name}",
  "description": "${mod_description}",
  "icon": "${mod_id}.icon.png",
  "authors": [
    "Moonflower"
  ],
  "contributors": [
    "Jackson",
    "RyanHCode"
  ],
  "contact": {
    "homepage": "https://www.moonflower.gg/",
    "issues": "https://github.com/MoonflowerTeam/${archives_base_name}/issues",
    "sources": "https://github.com/MoonflowerTeam/${archives_base_name}/"
  },
  "license": "GPLv3",
  "environment": "*",
  "entrypoints": {
    "main": [
      "gg.moonflower.carpenter.core.fabric.CarpenterFabric"
    ],
    "client": [
      "gg.moonflower.carpenter.core.fabric.CarpenterFabricClient"
    ],
    "fabric-datagen": [
      "gg.moonflower.carpenter.core.fabric.CarpenterFabricData"
    ]
  },
  "mixins": [
    "${mod_id}.mixins.json",
    "${mod_id}-common.mixins.json",
    {
      "config": "${mod_id}-common.client.mixins.json",
      "environment": "client"
    }
  ],
  "depends": {
    "fabric": "*",
    "minecraft": "~${minecraft_version}"
  }
}